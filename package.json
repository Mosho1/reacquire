{
  "name": "reacquire",
  "version": "0.1.1",
  "description": "Reload node modules",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha test/index.js --compilers js:babel-register",
    "test:watch": "mocha test/index.js -w --compilers js:babel-register",
    "build": "babel src --out-dir build",
    "build:watch": "babel src --out-dir build -w",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mosho1/reaquire.git"
  },
  "keywords": [
    "reload",
    "module",
    "require"
  ],
  "author": "Sharon Rolel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mosho1/reaquire/issues"
  },
  "homepage": "https://github.com/Mosho1/reaquire#readme",
  "devDependencies": {
    "babel-cli": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.3.13",
    "fs-promise": "^0.3.1",
    "mocha": "^2.3.4"
  },
  "dependencies": {
    "chokidar": "^1.4.0",
    "lodash.assign": "^3.2.0",
    "lodash.throttle": "^3.0.4"
  }
}
